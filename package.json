{
  "name": "iowho",
  "version": "1.0.0",
  "description": "Simple Money Lendings Tracker",
  "author": "Aaron Leo Cooper",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "6.5.0"
  },
  "scripts": {
    "start": "better-npm-run start",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "build:placeholder": "better-npm-run build:placeholder",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config webpack/config.hmr.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:dev": {
      "command": "gulp build:dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "gulp build:prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:placeholder": {
      "command": "gulp build:placeholder",
      "env": {
        "NODE_ENV": "placeholder"
      }
    },
    "test": {
      "command": "ava \"src/scripts/**/*.spec.js\" --verbose",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "ava \"src/scripts/**/*.spec.js\" --watch --verbose",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "axios": "0.14.0",
    "babel-core": "6.16.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "better-npm-run": "0.0.11",
    "classnames": "2.2.5",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-csso": "2.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-pug": "3.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.3.2",
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.10",
    "lodash": "4.16.6",
    "moment": "2.15.1",
    "nodemon": "1.10.2",
    "parallelshell": "2.0.0",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-addons-shallow-compare": "15.3.2",
    "react-click-outside": "2.2.0",
    "react-dom": "15.3.2",
    "react-helmet": "3.1.0",
    "react-hot-loader": "3.0.0-beta.5",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.6.0",
    "redux-localstorage": "0.4.1",
    "redux-logger": "2.7.0",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "run-sequence": "1.2.2",
    "webpack": "1.13.2",
    "webpack-strip": "0.1.0"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-plugin-rewire": "1.0.0",
    "babel-register": "6.16.3",
    "browser-sync": "2.17.6",
    "browser-sync-webpack-plugin": "1.1.3",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "4.18.2",
    "eslint-config-standard": "6.2.1",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-promise": "3.3.1",
    "eslint-plugin-react": "6.6.0",
    "eslint-plugin-standard": "2.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.6.0",
    "node-sass": "3.10.1",
    "react-addons-test-utils": "15.3.2",
    "rewire": "2.5.2",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack-dev-server": "1.16.1"
  },
  "ava": {
    "require": [
      "babel-register",
      "./src/scripts/helpers/setup-ava-tests.js"
    ],
    "babel": "inherit"
  }
}
